  "version": "0.2.0",
  "configurations": [
    {
      "name": "Main Run",
      "type": "python",
      "request": "launch",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}\\main.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Simulate Server",
      "type": "python",
      "request": "launch",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}\\main.py",
      "console": "integratedTerminal",
      "args": [
        "testing_name -lossf MME -discount 0.7 -lambd 0.5 -lr 0.0001 -dropout 0"
      ]
    },
    {
      "name": "Generate Experiments",
      "type": "python",
      "request": "launch",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}\\Utils\\generateExperiments.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Play Yourself",
      "type": "python",
      "request": "launch",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}\\environments.py",
      "console": "integratedTerminal"
    }
  ]
}
